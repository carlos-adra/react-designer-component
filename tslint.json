{
    "defaultSeverity": "error",
    "extends": [
        "tslint:latest",
        "tslint-react",
        "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {
        "ordered-imports": true,
        "quotemark": [
            true,
            "double"
        ],
        "no-unused-variable": true,
        "semicolon": [
            true,
            "always",
            "strict-bound-class-methods"
        ],
        "no-submodule-imports": false,
        "no-implicit-dependencies": false,
        "no-console": false,
        "member-access": true,
        "only-arrow-functions": true
    },
    "rulesDirectory": []
}